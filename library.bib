keywords = {Adult,Aged,Amino Acid Sequence,Biological Markers,Biological Markers: urine,Chromatography,Cluster Analysis,Female,Homologous,Humans,Kidney,Kidney Transplantation,Kidney: chemistry,Kidney: metabolism,Liquid,Liquid: methods,Male,Middle Aged,Molecular Sequence Data,Peptides,Peptides: chemistry,Peptides: urine,Phenotype,Primary Graft Dysfunction,Primary Graft Dysfunction: diagnosis,Primary Graft Dysfunction: therapy,Primary Graft Dysfunction: urine,Proteomics,Proteomics: methods,Reproducibility of Results,Tandem Mass Spectrometry,Tandem Mass Spectrometry: methods,Transplantation},
month = jul,
