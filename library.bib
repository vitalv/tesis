issn = {1545-7885},
journal = {PLoS biology},
keywords = {Animals,CHO Cells,Cadherins,Cadherins: metabolism,\textit{Candida albicans},\textit{Candida albicans}: physiology,Cells, Cultured,Chromatography, Affinity,Cricetinae,Cricetulus,Endocytosis,Fungal Proteins,Fungal Proteins: isolation {\&} purification,Fungal Proteins: metabolism,Humans,Protein Binding},
month = {mar},
