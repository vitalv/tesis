month = {may},
number = {10},
pages = {4007--21},
pmid = {236308},
title = {{High resolution two-dimensional electrophoresis of proteins.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2874754{\&}tool=pmcentrez{\&}rendertype=abstract},
