journal = {Journal of proteome research},
month = {jul},
pmid = {26139527},
title = {{The State of the Human Proteome in 2014/2015 as viewed through PeptideAtlas: enhancing accuracy and coverage through the AtlasProphet.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26139527},
year = {2015}
}
