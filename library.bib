pmid = {22159873},
title = {{TraML--a standard format for exchange of selected reaction monitoring transition lists.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3322582{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {11},
year = {2012}
}
