@article{Dai2013,
abstract = {Candida albicans is the most common opportunistic fungal pathogen and its apoptosis is inducible by environmental stress. Based on our previous finding that transcription factor Cap1p was involved in baicalein-induced apoptosis, the present study aimed to further clarify the role of Cap1p in apoptosis by observing the impact of CAP1 deletion on cell fate. It was found that apoptotic stimulation with amphotericin B, acetic acid and hydrogen peroxide increased the number of apoptotic and necrotic cells, caspase activity and the accumulation of reactive oxygen species, whereas it decreased the mitochondrial membrane potential and intracellular ATP level in the cap1$\Delta$/$\Delta$ mutant. The cell fate was, at least partly, caused by glutathione depletion and attenuation of the expression of the glutathione reductase gene in the cap1$\Delta$/$\Delta$ mutant. Collectively, our data suggest that Cap1p participated in the apoptosis of C. albicans by regulating the expression of the glutathione reductase gene and glutathione content.},
author = {Dai, Bao-Di and Wang, Yan and Zhao, Lan-Xue and Li, De-Dong and Li, Ming-Bang and Cao, Yong-Bing and Jiang, Yuan-Ying},
doi = {10.1111/febs.12251},
