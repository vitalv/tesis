pmid = {21385435},
title = {{compomics-utilities: an open-source Java library for computational proteomics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3060842{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {12},
