isbn = {1535-9484 (Electronic)$\backslash$r1535-9476 (Linking)},
issn = {1535-9476},
journal = {Molecular {\&} Cellular Proteomics},
keywords = {Algorithms,Amino Acid Sequence,Data Interpretation,Humans,Jurkat Cells,Peptide Fragments,Peptide Fragments: chemistry,Probability,Proteome,Proteome: chemistry,Proteomics,Search Engine,Software,Statistical,Streptococcus pyogenes,Tandem Mass Spectrometry},
