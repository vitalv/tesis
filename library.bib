journal = {Methods in molecular biology (Clifton, N.J.)},
keywords = {Databases, Protein,Internet,Molecular Sequence Annotation,Programming Languages,Proteome,Proteome: analysis,Proteomics,Proteomics: methods,User-Computer Interface},
