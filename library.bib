title = {{Extensive Mass Spectrometry-Based Analysis of the Fission Yeast Proteome: The S. pombe PeptideAtlas}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23462206 http://www.mcponline.org/content/early/2013/03/05/mcp.M112.023754.short},
