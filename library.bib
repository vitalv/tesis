journal = {Bioinformatics},
language = {en},
month = {nov},
number = {22},
pages = {2830--2832},
title = {{General framework for developing and evaluating database scoring algorithms using the TANDEM search engine}},
url = {http://bioinformatics.oxfordjournals.org/content/22/22/2830 http://bioinformatics.oxfordjournals.org/content/22/22/2830.abstract http://bioinformatics.oxfordjournals.org/content/22/22/2830.abstract?ijkey=9956e5c73dba428c0cb9cb15c28bca70d9e5d7e6{\&}keytype2=tf{\_}ipsecsha http://bioinformatics.oxfordjournals.org/content/22/22/2830.full.pdf http://bioinformatics.oxfordjournals.org/content/22/22/2830.short},
