journal = {Proteomics},
keywords = {Candida albicans,Candida albicans: chemistry,Candida albicans: metabolism,Cell Membrane,Cell Membrane: chemistry,Fungal Proteins,Fungal Proteins: analysis,Fungal Proteins: metabolism,Glycosylphosphatidylinositols,Glycosylphosphatidylinositols: metabolism,Membrane Proteins,Membrane Proteins: analysis,Membrane Proteins: metabolism,Proteome,Proteome: analysis,Proteome: metabolism,beta-Glucans,beta-Glucans: metabolism},
month = oct,
number = {20},
pages = {4770--86},
pmid = {19824013},
title = {{Analysis of Candida albicans plasma membrane proteome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19824013},
volume = {9},
year = {2009}
}
