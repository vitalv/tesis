mendeley-tags = {Algorithms,Chromatography- Liquid,Computational Biology,Databases- Protein,Humans,Hydrophobic and Hydrophilic Interactions,Internet,Molecular Weight,Peptides,Proteolysis,Reproducibility of Results,Saccharomyces cerevisiae,Sensitivity and Specificity,Software,Tandem Mass Spectrometry},
month = {apr},
