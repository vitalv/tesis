pages = {502--517},
pmid = {21133346},
title = {{Quantitative proteome and acidic subproteome profiling of \textit{Candida albicans} yeast-to-hypha transition}},
url = {http://dx.doi.org/10.1021/pr100710g http://pubs.acs.org/doi/full/10.1021/pr100710g http://pubs.acs.org/doi/pdfplus/10.1021/pr100710g http://pubs.acs.org/doi/abs/10.1021/pr100710g},
