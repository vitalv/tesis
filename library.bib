month = {jun},
number = {6},
pages = {2733--9},
pmid = {19326923},
title = {{Expediting the development of targeted SRM assays: using data from shotgun proteomics to automate method development.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2743471{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {8},
