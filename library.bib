journal = {Molecular systems biology},
keywords = {Amino Acids,Amino Acids: metabolism,Carbon,Carbon: metabolism,Cluster Analysis,Metabolic Networks and Pathways,Models, Biological,Proteomics,Proteomics: methods,Saccharomyces cerevisiae,Saccharomyces cerevisiae Proteins,Saccharomyces cerevisiae Proteins: analysis,Saccharomyces cerevisiae Proteins: chemistry,Saccharomyces cerevisiae Proteins: metabolism,Saccharomyces cerevisiae: enzymology,Saccharomyces cerevisiae: metabolism,Systems Biology,Systems Biology: methods},
month = {feb},
